(f 3 5)

#Recursive label demo
((label fibnL (lambda (n)
  (cond ((= n 0) 0)
        ((= n 1) 1)
	((= n 2) 1)
	(T (+ (fibnL (- n 1))
	      (fibnL (- n 2))))))) 7)