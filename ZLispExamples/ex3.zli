#Recursive defun example
(defun fibn (n)
  (cond ((= n 0) 0)
        ((= n 1) 1)
	((= n 2) 1)
	(T (+ (fibn (- n 1))
	      (fibn (- n 2))))))
	      
(fibn 7)

